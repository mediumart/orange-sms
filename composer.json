{
    "name": "mediumart/orange-sms",
    "type": "library",
    "license": "MIT",
    "keywords": ["orange", "orange-api", "sms"],
    "description": "A php library to interact with the orange sms api for MiddleEast and Africa.",
    "authors": [
        {
            "name": "Isaac Esso",
            "email": "daprod2009@gmail.com"
        }
    ],
    "require": {
        "php": ">= 5.6",
        "guzzlehttp/guzzle": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Mediumart\\Orange\\SMS\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orange\\SMS\\Tests\\": "tests/"
        }
    }
}
